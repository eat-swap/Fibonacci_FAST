#: fastfib.py:21
msgid "This program is used to calculate Fibonacci number."
msgstr "此程式用於計算費式數列。"

#: fastfib.py:22
msgid "Please report any bugs to <https://github.com/lamhaoyin/Fibonacci_FAST>"
msgstr "請回報臭蟲到 <https://github.com/lamhaoyin/Fibonacci_FAST>"

#: fastfib.py:29
msgid "the N value of F(N)."
msgstr "F(N) 的 N 值。"

#: fastfib.py:36
msgid "Verbose mode"
msgstr "詳細輸出模式"

#: fastfib.py:52
#, python-format
msgid "[I] Start process: %s"
msgstr "[I] 開始程序：%s"

#: fastfib.py:59
#, python-format
msgid "[I] Now process: %d"
msgstr "[I] 目前進度：%d"

#: fastfib.py:62
#, python-format
msgid ""
"[I] Done. F(n) is:\n"
"%d"
msgstr ""
"[I] 完成，F(n) 為：\n"
"%d"

#: /usr/lib/python3.7/argparse.py:297
msgid "usage: "
msgstr "用法："

#: /usr/lib/python3.7/argparse.py:839
msgid ".__call__() not defined"
msgstr "未定義 .__call__()"

#: /usr/lib/python3.7/argparse.py:1142
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "未知解析器 %(parser_name)r（選擇：%(choices)s）"

#: /usr/lib/python3.7/argparse.py:1196
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "使用 %r 模式的參數「-」"

#: /usr/lib/python3.7/argparse.py:1204
#, python-format
msgid "can't open '%s': %s"
msgstr "無法開啟「%s」：%s"

#: /usr/lib/python3.7/argparse.py:1408
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "無法合併動作 - 兩個群組皆命名為 %r"

#: /usr/lib/python3.7/argparse.py:1446
msgid "'required' is an invalid argument for positionals"
msgstr "「required」參數對必要參數無效"

#: /usr/lib/python3.7/argparse.py:1468
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr "無效的選項字串 %(option)r：必須以字元 %(prefix_chars)r 開頭"

#: /usr/lib/python3.7/argparse.py:1488
#, python-format
msgid "dest= is required for options like %r"
msgstr "對類似 %r 的選項而言，dest= 必須設定"

#: /usr/lib/python3.7/argparse.py:1505
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "無效的 conflict_resolution 值：%r"

#: /usr/lib/python3.7/argparse.py:1523
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "選項字串衝突：%s"

#: /usr/lib/python3.7/argparse.py:1589
msgid "mutually exclusive arguments must be optional"
msgstr "相互排斥參數必須為「選用參數」"

#: /usr/lib/python3.7/argparse.py:1652
msgid "positional arguments"
msgstr "必要參數"

#: /usr/lib/python3.7/argparse.py:1653
msgid "optional arguments"
msgstr "選用參數"

#: /usr/lib/python3.7/argparse.py:1668
msgid "show this help message and exit"
msgstr "顯示此說明訊息後退出"

#: /usr/lib/python3.7/argparse.py:1699
msgid "cannot have multiple subparser arguments"
msgstr "不可包含多個 subparser 參數"

#: /usr/lib/python3.7/argparse.py:1751 /usr/lib/python3.7/argparse.py:2257
#, python-format
msgid "unrecognized arguments: %s"
msgstr "無法識別的參數：%s"

#: /usr/lib/python3.7/argparse.py:1848
#, python-format
msgid "not allowed with argument %s"
msgstr "不允許傳入參數 %s"

#: /usr/lib/python3.7/argparse.py:1894 /usr/lib/python3.7/argparse.py:1908
#, python-format
msgid "ignored explicit argument %r"
msgstr "已忽略明確參數 %r"

#: /usr/lib/python3.7/argparse.py:2015
#, python-format
msgid "the following arguments are required: %s"
msgstr "以下參數必須傳入：%s"

#: /usr/lib/python3.7/argparse.py:2030
#, python-format
msgid "one of the arguments %s is required"
msgstr "必須傳入 %s 參數之一"

#: /usr/lib/python3.7/argparse.py:2073
msgid "expected one argument"
msgstr "期望一個參數"

#: /usr/lib/python3.7/argparse.py:2074
msgid "expected at most one argument"
msgstr "期望最多一個參數"

#: /usr/lib/python3.7/argparse.py:2075
msgid "expected at least one argument"
msgstr "期望最少一個參數"

#: /usr/lib/python3.7/argparse.py:2077
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "期望有 %s 參數"

#: /usr/lib/python3.7/argparse.py:2137
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "選項不明確：%(option)s 也能與 %(matches)s 配合使用"

#: /usr/lib/python3.7/argparse.py:2200
#, python-format
msgid "unexpected option string: %s"
msgstr "非期望的選項字串：%s"

#: /usr/lib/python3.7/argparse.py:2397
#, python-format
msgid "%r is not callable"
msgstr "%r 無法被呼叫"

#: /usr/lib/python3.7/argparse.py:2414
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "無效的 %(type)s 值：%(value)r"

#: /usr/lib/python3.7/argparse.py:2425
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "選擇無效：%(value)r（從 %(choices)s 選擇）"

#: /usr/lib/python3.7/argparse.py:2501
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s：錯誤：%(message)s\n"
